{
  "name": "roster-mobile",
  "version": "1.0.0",
  "description": "Norwegian Labor Law Compliant Roster Mobile App",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
