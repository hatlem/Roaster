    "lint": "eslint .",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace RosterMobile.xcworkspace -scheme RosterMobile -configuration Release"
  },
  "dependencies": {
